# staticprops
WebLogicServerChoice=0
WebSphereServerChoice=0
TomcatChoice=1
APPSERVER_TOMCAT={{ tomcat_root }}/asmserver1
USER_INSTALL_DIR={{ ariba_base }}
INSTALLER_PROPERTIES_FILE_PATH={{ ariba_conf }}/upstream-installer.properties
Install_Only=false
domain.directory={{ ariba_root }}/bea/user_projects
appserver.home={{ ariba_root }}/bea
bea.home={{ ariba_root }}/bea
admin.port=7001
# j2eesetup always creates user, but weblogic exists by default
# TODO: why isnt user=system defaulted?
admin.user=system
admin.host={{ ariba_host }}
# has to contain at least one digit and mixed case
admin.password=Weblogic0000
ASMSharedSecret=tool
CNCIdentity=
CNCSharedSecret=tool

# devprops
oracle.jdbc.driver={{ ariba_inst }}/database/ojdbc6.jar


# custprops
domain=Sourcing
asm.group=
server1.name=asmserver1
server1.port=8000
server1.initialheap=256
server1.maxheap=512
server1.addtl-mem-args=-Xss1m -XX:MaxPermSize=256M -Xdebug -Xrunjdwp:transport=dt_socket,server=y,suspend=n,address=5005 -noverify 
server2.name=asmserver2
server2.port=8004
server2.initialheap=256
server2.maxheap=512
server2.addtl-mem-args=-Xss1m -XX:MaxPermSize=256M
sourcing.configure.customize.webserverport=80
sourcing.configure.customize.webserverhostname={{ ariba_host }}
sourcing.configure.freshconfig=true
sourcing.configure.aribadbvalidateconnection=false
sourcing.home={{ ariba_base }}
sourcing.server.home={{ ariba_server }}
sourcing.web.home={{ ariba_base }}/WebComponents
sourcing.configure.customize.virtualdir=AribaASM

Application.Base.Data.DefaultLanguage=English
Application.Base.Data.DefaultCurrency=EUR
Application.Base.Data.DefaultOrganicUserLocale=en_US
Application.Reports.CSVReportsCharset=UTF-8
Application.Base.Data.BaseLanguage=U.S English
System.Base.DefaultLocale=en_US
System.Base.InstanceName=ACM

// the following 2 are not set
System.Base.ASMShared="{{ ariba_root }}/Shared"
System.Base.Doc="{{ ariba_server }}/DocRoot"

System.Base.SMTPServerNameList=localhost
System.Base.Data.BaseCurrency=EUR
System.Base.Data.ReportingCurrency=EUR
System.Base.Modules = ( Basic, Admin, Collaborate, Sourcing, Analytics );
System.Base.ResourceURL="/AribaASM"

